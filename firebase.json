{
  "hosting": {
    "public": "target/doc",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
       {
	 "source": "/",
	 "destination": "/zebrad",
	 "type": 301
       }
    ]
  }
}
